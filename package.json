{
	"name": "spaceload",
	"version": "0.0.11",
	"description": "Create a system of namespace with an integrated autoloader",
	"main": "index.js",
	"engines": {
		"node": ">=0.10.0"
	},
	"dependencies": {
		"silex-glob": "<2.0.0",
		"commander": "2.x"
	},
	"scripts": {
		"test": "node ./bin/spaceload install ./test --test && node ./test/test.js"
	},
	"bin": {
		"spaceload" : "./bin/spaceload"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/silexjs/spaceload.git"
	},
	"keywords": [
		"spaceload",
		"namespace",
		"autoload",
		"autoloader",
		"silex"
	],
	"author": "Valentin Allaire <valentin@sitexw.fr>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/silexjs/spaceload/issues"
	},
	"homepage": "https://github.com/silexjs/spaceload",
	"directories": {
		"test": "test"
	},
	"autoload": {
		"classmap": {
			"Silex\\Spaceload": "./index.js"
		}
	}
}
